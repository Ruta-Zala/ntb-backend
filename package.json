{
  "name": "networking-toolbox-parietus-api",
  "version": "1.0.1",
  "type": "module",
  "description": "public-facing REST API to support networking-toolbox UI",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index",
    "dev": "nodemon src/index.js",
    "seed-user": "node src/seeder/User-seeder.js",
    "seed-website": "node src/seeder/WebsiteSettingsSeeder.js",
    "test:unit": "jest tests/unit --runInBand --coverage",
    "test:e2e": "jest tests/e2e --runInBand --coverage",
    "lint": "eslint src/ --ignore-pattern 'src/tests/*' --fix",
    "format": "prettier --write src/",
    "prepare": "husky",
    "format-check": "eslint --fix --max-warnings 0 && prettier --check src/"
  },
  "keywords": [],
  "author": "Melinda Ralston <meralsto@cisco.com>",
  "contributors": [
    "Ruta Zala <ruzala@cisco.com>"
  ],
  "license": "ISC",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^4.3.1",
    "mongoose": "^8.8.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.15.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-mock-req-res": "^1.0.2",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  }
}
